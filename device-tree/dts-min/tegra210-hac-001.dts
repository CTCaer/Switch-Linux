	clocks {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		clk32k_in: clock@0 {
			compatible = "fixed-clock";
			reg = <0>;
			#clock-cells = <0>;
			clock-frequency = <32768>;
		};
	};
	
	// Horizon OS's TrustZone component uses PSCIv0.1 with custom IDs.
    // Additionally, it uses SMC #1 instead of SMC #0
    psci {
        compatible = "arm,psci";
		status = "okay";
		method = "smc";
		cpu_suspend	= <0xC4000001>;
		cpu_off		= <0x84000002>;
		cpu_on		= <0xC4000003>;
	};

	cpus {
		cpu@0 {
			enable-method = "psci";
			status = "okay";
		};

		cpu@1 {
			enable-method = "psci";
			status = "okay";
		};

		cpu@2 {
			enable-method = "psci";
			status = "okay";
		};

		cpu@3 {
			enable-method = "psci";
			status = "okay";
		};
	};

    gpio-keys {
		compatible = "gpio-keys";
		gpio-keys,name = "gpio-keys";

		power {
			label = "Power";
			gpios = <&gpio TEGRA_GPIO(X, 5) GPIO_ACTIVE_LOW>;
			linux,code = <KEY_POWER>;
			debounce-interval = <30>;
			wakeup-source;
		};

		volume_down {
			label = "Volume Down";
			gpios = <&gpio TEGRA_GPIO(X, 7) GPIO_ACTIVE_LOW>;
			linux,code = <KEY_VOLUMEDOWN>;
		};

		volume_up {
			label = "Volume Up";
			gpios = <&gpio TEGRA_GPIO(X, 6) GPIO_ACTIVE_LOW>;
			linux,code = <KEY_VOLUMEUP>;
		};
	};

	regulators {
		compatible = "simple-bus";
		device_type = "fixed-regulators";
		#address-cells = <1>;
		#size-cells = <0>;

		ppvar_sys: regulator@0 {
			compatible = "regulator-fixed";
			reg = <0>;
			regulator-name = "PPVAR_SYS";
			regulator-min-microvolt = <4400000>;
			regulator-max-microvolt = <4400000>;
			regulator-always-on;
		};

		pp3000_always: regulator@2 {
			compatible = "regulator-fixed";
			reg = <2>;
			regulator-name = "PP3000_ALWAYS";
			regulator-min-microvolt = <3000000>;
			regulator-max-microvolt = <3000000>;
			regulator-always-on;
		};

		pp3300: regulator@3 {
			compatible = "regulator-fixed";
			reg = <3>;
			regulator-name = "PP3300";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-boot-on;
			regulator-always-on;
			enable-active-high;
		};

		pp5000: regulator@4 {
			compatible = "regulator-fixed";
			reg = <4>;
			regulator-name = "PP5000";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
		};

        max77620_gpio7: regulator@5 {
			compatible = "regulator-fixed";
			reg = <5>;
			regulator-name = "max77620-gpio7";
			regulator-min-microvolt = <1200000>;
			regulator-max-microvolt = <1200000>;
			regulator-boot-on;
			gpio = <&max77620 7 0>;
			enable-active-high;
			regulator-always-on;
			regulator-enable-ramp-delay = <240>;
			regulator-disable-ramp-delay = <11340>;
		};

		vdd_3v3_sd: regulator@6 {
			compatible = "regulator-fixed";
			reg = <6>;
			regulator-name = "VDD_3V3_SD";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			gpio = <&gpio TEGRA_GPIO(E, 4) GPIO_ACTIVE_HIGH>;
			enable-active-high;
			vin-supply = <&pp3300>;

			regulator-enable-ramp-delay = <472>;
			regulator-disable-ramp-delay = <4880>;
		};

		usbc_vbus: regulator@7 {
			compatible = "regulator-fixed";
			reg = <7>;
			regulator-name = "USBC_VBUS";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
		};

		pp5000_bl_avdd: regulator@11 {
			compatible = "regulator-fixed";
			reg = <11>;
			regulator-name = "pp5000_bl_avdd";
			regulator-min-microvolt = <5400000>;
			regulator-max-microvolt = <5400000>;
		};
		
        lcd_dummy: regulator@12 {
			compatible = "regulator-fixed";
			reg = <12>;
			regulator-name = "LCD_DUMMY";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};
	};

    host1x@50000000 {
		dsi@54300000 {
			status = "okay";

			avdd-dsi-csi-supply = <&lcd_dummy>;

			panel@0 {
				compatible = "nintendo,hac-720p";
				reg = <0>;

                bl-gpio = <&gpio TEGRA_GPIO(V, 0) GPIO_ACTIVE_HIGH>;
				enable-gpio = <&gpio TEGRA_GPIO(V, 1) GPIO_ACTIVE_HIGH>;
				reset-gpio = <&gpio TEGRA_GPIO(V, 2) GPIO_ACTIVE_HIGH>;
				pvdd-gpio = <&gpio TEGRA_GPIO(I, 0) GPIO_ACTIVE_HIGH>;
				nvdd-gpio = <&gpio TEGRA_GPIO(I, 1) GPIO_ACTIVE_HIGH>;
				panel-boot-on;
			};
		};
	};
};
