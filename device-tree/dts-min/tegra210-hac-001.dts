	/* eMMC */
	sdhci@700b0600 {
		status = "okay";
		bus-width = <8>;
		non-removable;
	};

	sdhci@700b0000 {
		status = "okay";
		bus-width = <4>;
		no-1-8-v;

		cd-gpios = <&gpio TEGRA_GPIO(Z, 1) GPIO_ACTIVE_LOW>;

		vqmmc-supply = <&pp_ldo2>;
		vmmc-supply = <&vdd_3v3_sd>;
	};

	aconnect@702c0000 {
		status = "okay";

		dma@702e2000 {
			status = "okay";
		};

		agic@702f9000 {
			status = "okay";
		};
